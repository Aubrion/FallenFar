            <string>/// Skills

// Check if skill triggers are pressed
if (obj_weapon_parent.state != 2)
{
    for (var i = 0; i &lt; array_length_1d(lynx_skill_trigger); i++)
    {
        if (keyboard_check_pressed(lynx_skill_trigger[i]))
