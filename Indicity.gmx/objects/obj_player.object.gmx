                case 6: var skill_data = lynx_skill6; break;
                case 7: var skill_data = lynx_skill7; break;
                case 8: var skill_data = lynx_skill8; break;
                default: var skill_data = false; break;
            }
            
            // Drop lynx instance
            if (array_height_2d(skill_data) &gt; 0)
            {
                var inst = instance_create(x, y, obj_lynx);
                inst.skill = skill_data;
                break; // Prevent multiple skills starting at the same step
            }
        }
    }
}
