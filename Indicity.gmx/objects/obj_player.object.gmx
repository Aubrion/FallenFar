                case 6: skill_data = lynx_skill6; break;
                case 7: skill_data = lynx_skill7; break;
                case 8: skill_data = lynx_skill8; break;
                default: skill_data = false; break;
            }

            // Drop lynx instance
            if (array_height_2d(skill_data) &gt; 0)
            {
                // Create instance
                var inst = instance_create(x, y, obj_lynx);
                with (inst) {skill = skill_data; parent = other.id;}

                // Start weapon attack
                obj_weapon_parent.state = 2;

                // Prevent multiple skills starting at the same step
                skill_activated = true;
                break;
            }
        }
    }

    // Check for basic attack if no skill has been activated this step
    if (!skill_activated &amp;&amp; keyboard_check(lynx_skill_trigger[0]))
