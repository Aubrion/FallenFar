            <string>///Collision based on Mass
var cx,cy,vx,vy,a,xoff,yoff,om,mm,mag;

cx = bbox_left + ((bbox_right- bbox_left)/2)
cy = bbox_bottom + ((bbox_top- bbox_bottom)/2)
vx = other.bbox_left + ((other.bbox_right- other.bbox_left)/2)
vy = other.bbox_bottom + ((other.bbox_top- other.bbox_bottom)/2)
a = point_direction(cx, cy, vx, vy);    //Get the direction from the colliding object
xoff = lengthdir_x(1, a);                       //Get the offset vector
yoff = lengthdir_y(1, a);
om = other.mass / mass;                         //start the fake "physics mass" calculations
mm = mass / other.mass;
mag = sqrt((om * om) + (mm * mm));
om /= mag;
mm /= mag;



//Move out of collision
while (place_meeting(x, y, other))
{
x -= xoff * om; //Move the instance out of collision
y -= yoff * om;
other.x += xoff * mm; //Move the other instance out of the collision
other.y += yoff * mm;
}


